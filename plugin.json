{
  "id": "nodebb-plugin-best-registration",
  "name": "best-registration",
  "description": "מוסיף שדות לרישום משתמשים.",
  "url": "https://github.com/yourusername/best-registration",
  "library": "./library.js",
  "hooks": [
	{
		"hook": "filter:register.build", "method": "addCaptcha"
	},
	{
		"hook": "filter:register.check", "method": "checkRegister"
	},
  {
    "hook": "action:user.create", "method": "saveBusinessData"
  }
  ],
  "nbbpm": {
    "compatibility": "^1.17.0"
  }
}
